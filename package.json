{
  "name": "temporal-hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bot": "ts-node src/playground/bot/src/index",
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "lint": "eslint .",
    "start": "ts-node src/temporal/worker.ts",
    "start.watch": "nodemon --watch '**/*' --ignore 'node_modules/' -e 'ts' --exec 'ts-node --files -r tsconfig-paths/register' src/temporal/worker.ts",
    "server": "nodemon --watch '**/*' --ignore 'node_modules/' -e 'ts' --exec 'ts-node --files -r tsconfig-paths/register' src/server/server.ts",
    "workflow": "ts-node src/client.ts",
    "testWorkflow": "ts-node src/testClient.ts",
    "utils:genAccounts": "ts-node src/scripts/genAccounts",
    "utils:genHdAccounts": "ts-node src/scripts/genHdAccounts",
    "utils:derivePrivateKeyFromMnemonic": "ts-node src/scripts/derivePrivateKeyFromMnemonic",
    "Polygon:readBalance": "ts-node src/scripts/Polygon/readBalance",
    "Polygon:readERC20Balance": "ts-node src/scripts/Polygon/readERC20Balance",
    "Polygon:sendTxn": "ts-node src/scripts/Polygon/sendTxn",
    "Polygon:wrapMatic": "ts-node src/scripts/Polygon/wrapMatic",
    "Polygon:upwrapMatic": "ts-node src/scripts/Polygon/upwrapMatic",
    "HoudiniSwap:obfuscate": "ts-node src/scripts/HoudiniSwap/obfuscate",
    "Uniswap:getLiquidityPair": "ts-node src/scripts/Polygon/UniswapV2/getLiquidityPair",
    "Uniswap:searchForLiquidity": "ts-node src/scripts/Polygon/UniswapV2/searchForLiquidity",
    "Uniswap:swapTokens": "ts-node src/scripts/Polygon/UniswapV2/swapTokens"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "_moduleAliases": {
    "@": "."
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/units": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "@sendgrid/mail": "^8.1.1",
    "@temporalio/activity": "^1.8.4",
    "@temporalio/client": "^1.8.4",
    "@temporalio/worker": "^1.8.4",
    "@temporalio/workflow": "^1.8.4",
    "@types/node-fetch": "^2.6.4",
    "@types/seedrandom": "^3.0.6",
    "@uniswap/sdk-core": "^4.0.10",
    "@uniswap/v3-sdk": "^3.10.2",
    "axios": "^1.6.2",
    "bignumber.js": "^9.1.2",
    "cors": "^2.8.5",
    "decimal": "^0.0.2",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ejson": "^2.2.3",
    "ethers": "^6.10.0",
    "express": "^4.18.2",
    "lodash.get": "^4.4.2",
    "migrate-mongo": "^11.0.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.2.0",
    "mongoose-unique-validator": "^5.0.0",
    "nanoid": "3.x",
    "node-fetch": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/ejson": "^2.2.2",
    "@types/express": "^4.17.21",
    "@types/lodash.get": "^4.4.9",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-unique-validator": "^1.0.9",
    "@types/node": "^16.11.43",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "body-parser": "^1.20.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "mongoose": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "seedrandom": "^3.0.5",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.4.2"
  },
  "description": "This project is referred to as \"The Airdrop Bot\". This project securely generates blockchain accounts and simulates humanlike transactions with them over long periods of time to bring the accounts into a state where they can qualify for token rewards on various EVM-based chains.",
  "main": ".eslintrc.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
